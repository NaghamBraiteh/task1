import React, { useState, useEffect } from "react";

function TodoList() {
  const [todos, setTodos] = useState() 
  useEffect(() => {
    Document.title='todos:$(todos)'
    console.log('Todos has been  updated to :$(todos)');
  });

  return (
    <div>
      <h2>Todos:{Todos}</h2>
    </div>
  );
}

